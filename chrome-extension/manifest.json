{
  "manifest_version": 2,

  "name": "SuperTeamAwesome Toolbar",
  "description": "MIT Mystery Hunt Helper",
  "version": "1.0",

  "background": {
    "scripts": [
      "firebase-3.6.2.js",
      "config.js",
      "background.js"
    ]
  },

  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "popup/popup.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "content_script.js"
      ]
    }
  ],

  "content_security_policy": "script-src 'self' https://*.firebaseio.com; object-src 'self'",

  "key": "",

  "oauth2": {
    "client_id": "941573006926-dbr8496lpv0oq37fkm9h6p6e3nabgd2l.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },

  "permissions": [
    "identity"
  ],

  "web_accessible_resources": [
    "toolbar/toolbar.html"
  ]
}
