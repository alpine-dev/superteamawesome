{
  "name": "superteamawesome-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:copy": "npm-run-all build:copy:folder build:copy:images build:copy:fonts",
    "build:copy:folder": "mkdir -p server/public",
    "build:copy:images": "cp -r app/src/images server/public",
    "build:copy:fonts": "cp -r app/src/fonts server/public",
    "build:compile": "tsc -p server/",
    "start": "node ./server/app.js",
    "start:dev": "nodemon ./server/app.js",
    "start:prod": "npm-run-all dist start",
    "dev": "npm-run-all build:copy build:compile dev:run",
    "dev:run": "npm-run-all -p watch start:dev",
    "clean": "rm -rf server/public/*",
    "dist": "npm-run-all clean:public build:copy build:compile dist:bundle:webpack",
    "dist:bundle:webpack": "webpack --config config/webpack.config.prod.js",
    "setup": "npm-run-all setup:install build:compile",
    "setup:install": "npm install -g webpack typescript tslint nodemon",
    "watch": "npm-run-all -p watch:app watch:server",
    "watch:app": "webpack --watch --profile --colors --progress --config config/webpack.config.js",
    "watch:server": "tsc -w -p server/"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "classnames": "2.2.5",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "es6-shim": "^0.35.3",
    "express": "^4.14.0",
    "firebase": "^3.6.4",
    "google-client-api": "^1.0.4",
    "hbs": "~4.0.1",
    "history": "^4.7.2",
    "moment": "^2.17.1",
    "morgan": "~1.7.0",
    "normalize.css": "^5.0.0",
    "react": "^15.6.1",
    "react-addons-update": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-google-login": "^2.5.9",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/classnames": "0.0.32",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.34",
    "@types/node": "^6.0.58",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.20",
    "@types/react-router-dom": "^4.2.3",
    "@types/redux-logger": "^3.0.0",
    "@types/redux-thunk": "^2.1.32",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "chokidar-cli": "^1.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^4.0.5",
    "less-plugin-npm-import": "^2.1.0",
    "npm-run-all": "^3.1.2",
    "postcss": "^5.2.6",
    "postcss-less": "^0.15.0",
    "postcss-loader": "^1.2.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.2",
    "webpack": "^2.5.1"
  }
}
