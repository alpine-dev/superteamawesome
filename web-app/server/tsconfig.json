{
  "compilerOptions": {
    "declaration": false,
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "rootDir": ".",
    "sourceMap": false,
    "stripInternal": true,
    "target": "es6"
  }
}
